alias ssh_add='eval $(ssh-agent) && ssh-add ~/.ssh/id_ed25519'

no_machine()
{
  /Applications/NoMachine.app/Contents/MacOS/nxplayer --session kklochkov-bbox.nxs & disown && exit
}

export VISUAL=nvim
export EDITOR="$VISUAL"

alias exa_ls="exa -ahl"

plugins=(
  git
  colorize
  colored-man-pages
  zsh-syntax-highlighting
  zsh-interactive-cd
  zsh-navigation-tools
  zsh-autosuggestions
)
